    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "type": "timeseries",
      "title": "Stability Trend Over Time",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  UNIX_TIMESTAMP(DATE_FORMAT(updated_at, '%Y-%m-%d %H:%i:00')) as time_sec,\n  COUNT(CASE WHEN stability_status = 'Stable' THEN 1 END) as stable,\n  COUNT(CASE WHEN stability_status = 'UnstableHourly' THEN 1 END) as unstable_hourly,\n  COUNT(CASE WHEN stability_status = 'UnstableDaily' THEN 1 END) as unstable_daily\nFROM proxies\nWHERE active = true AND updated_at >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\nGROUP BY time_sec\nORDER BY time_sec",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "type": "table",
      "title": "Top Unstable Proxies",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  name,\n  location,\n  stability_status,\n  last_ip,\n  same_ip_count,\n  rotation_count,\n  updated_at\nFROM proxies\nWHERE active = true AND stability_status IN ('UnstableHourly', 'UnstableDaily')\nORDER BY updated_at DESC\nLIMIT 20",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "type": "bargauge",
      "title": "Stability Status by Location",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  location,\n  stability_status,\n  COUNT(*) as count\nFROM proxies\nWHERE active = true\nGROUP BY location, stability_status\nORDER BY location, stability_status",
          "format": "table",
          "refId": "A"
        }
      ]
    }
  ],
