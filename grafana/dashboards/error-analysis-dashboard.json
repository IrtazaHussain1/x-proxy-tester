{
  "title": "XProxy Tester - Error Analysis",
  "tags": [
    "x-proxy-tester",
    "errors"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "type": "stat",
      "title": "Total Errors (24h)",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT COUNT(*) as value FROM proxy_requests WHERE timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR) AND status != 'SUCCESS'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 1000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "type": "stat",
      "title": "Error Rate",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT COUNT(CASE WHEN status != 'SUCCESS' THEN 1 END) * 100.0 / COUNT(*) as value FROM proxy_requests WHERE timestamp >= DATE_SUB(NOW(), INTERVAL 1 HOUR)",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "type": "stat",
      "title": "Unique Error Types",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT COUNT(DISTINCT error_type) as value FROM proxy_requests WHERE timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR) AND error_type IS NOT NULL",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          }
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "type": "stat",
      "title": "Top Failing Proxy",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  p.name as value\nFROM proxies p\nJOIN proxy_requests pr ON p.device_id = pr.proxy_id\nWHERE pr.timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\n  AND pr.status != 'SUCCESS'\nGROUP BY p.device_id, p.name\nORDER BY COUNT(*) DESC\nLIMIT 1",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          }
        }
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "type": "timeseries",
      "title": "Error Rate Over Time",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  UNIX_TIMESTAMP(DATE_FORMAT(timestamp, '%Y-%m-%d %H:%i:00')) as time_sec,\n  COUNT(CASE WHEN status != 'SUCCESS' THEN 1 END) * 100.0 / COUNT(*) as error_rate\nFROM proxy_requests\nWHERE timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\nGROUP BY time_sec\nORDER BY time_sec",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "type": "piechart",
      "title": "Error Type Distribution",
      "datasource": {
        "type": "mysql",
        "uid": "xproxy-tester-mysql"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            }
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "pieType": "pie",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": true
        },
        "sort": "desc",
        "tooltip": {
          "hideZeros": false,
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "12.3.0",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  COALESCE(error_type, 'UNKNOWN') as metric,\n  COUNT(*) as value\nFROM proxy_requests\nWHERE timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\n  AND status != 'SUCCESS'\nGROUP BY error_type",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "type": "table",
      "title": "Top 20 Failing Proxies",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  p.name,\n  p.location,\n  COUNT(CASE WHEN pr.status != 'SUCCESS' THEN 1 END) as error_count,\n  COUNT(*) as total_requests,\n  COUNT(CASE WHEN pr.status != 'SUCCESS' THEN 1 END) * 100.0 / COUNT(*) as error_rate,\n  GROUP_CONCAT(DISTINCT pr.error_type) as error_types\nFROM proxies p\nJOIN proxy_requests pr ON p.device_id = pr.proxy_id\nWHERE pr.timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\nGROUP BY p.device_id, p.name, p.location\nHAVING error_count > 0\nORDER BY error_count DESC\nLIMIT 20",
          "format": "table",
          "refId": "A"
        }
      ]
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "weekStart": ""
}
