{
  "title": "XProxy Tester - Performance Metrics",
  "tags": [
    "x-proxy-tester",
    "performance"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "1m",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "type": "stat",
      "title": "Avg Response Time (P50)",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT AVG(response_time_ms) as value FROM proxy_requests WHERE timestamp >= DATE_SUB(NOW(), INTERVAL 1 HOUR) AND response_time_ms IS NOT NULL",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1000,
                "color": "yellow"
              },
              {
                "value": 3000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "type": "stat",
      "title": "P95 Response Time",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  response_time_ms as value\nFROM (\n  SELECT\n    response_time_ms,\n    ROW_NUMBER() OVER (ORDER BY response_time_ms) as row_num,\n    COUNT(*) OVER () as total_count\n  FROM proxy_requests\n  WHERE timestamp >= DATE_SUB(NOW(), INTERVAL 1 HOUR)\n    AND response_time_ms IS NOT NULL\n) ranked\nWHERE row_num = FLOOR(total_count * 0.95) + 1\nLIMIT 1",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 2000,
                "color": "yellow"
              },
              {
                "value": 5000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "type": "stat",
      "title": "P99 Response Time",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  response_time_ms as value\nFROM (\n  SELECT\n    response_time_ms,\n    ROW_NUMBER() OVER (ORDER BY response_time_ms) as row_num,\n    COUNT(*) OVER () as total_count\n  FROM proxy_requests\n  WHERE timestamp >= DATE_SUB(NOW(), INTERVAL 1 HOUR)\n    AND response_time_ms IS NOT NULL\n) ranked\nWHERE row_num = FLOOR(total_count * 0.99) + 1\nLIMIT 1",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 3000,
                "color": "yellow"
              },
              {
                "value": 10000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "type": "stat",
      "title": "Requests per Minute",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT COUNT(*) / 60.0 as value FROM proxy_requests WHERE timestamp >= DATE_SUB(NOW(), INTERVAL 1 HOUR)",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          }
        }
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "type": "timeseries",
      "title": "Response Time Distribution (P50, P95, P99)",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  UNIX_TIMESTAMP(DATE_FORMAT(timestamp, '%Y-%m-%d %H:%i:00')) as time_sec,\n  AVG(response_time_ms) as p50,\n  MAX(response_time_ms) as p99\nFROM proxy_requests\nWHERE timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\n  AND response_time_ms IS NOT NULL\nGROUP BY time_sec\nORDER BY time_sec",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "type": "histogram",
      "title": "Response Time Histogram",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  CASE\n    WHEN response_time_ms < 500 THEN '< 500ms'\n    WHEN response_time_ms < 1000 THEN '500-1000ms'\n    WHEN response_time_ms < 2000 THEN '1000-2000ms'\n    WHEN response_time_ms < 5000 THEN '2000-5000ms'\n    ELSE '> 5000ms'\n  END as bucket,\n  COUNT(*) as count\nFROM proxy_requests\nWHERE timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\n  AND response_time_ms IS NOT NULL\nGROUP BY bucket\nORDER BY bucket",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "type": "table",
      "title": "Top 20 Proxies by Average Response Time",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  p.name,\n  p.location,\n  AVG(pr.response_time_ms) as avg_response_time,\n  COUNT(*) as request_count,\n  COUNT(CASE WHEN pr.status = 'SUCCESS' THEN 1 END) * 100.0 / COUNT(*) as success_rate\nFROM proxies p\nJOIN proxy_requests pr ON p.device_id = pr.proxy_id\nWHERE pr.timestamp >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\n  AND pr.response_time_ms IS NOT NULL\nGROUP BY p.device_id, p.name, p.location\nORDER BY avg_response_time DESC\nLIMIT 20",
          "format": "table",
          "refId": "A"
        }
      ]
    }
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
