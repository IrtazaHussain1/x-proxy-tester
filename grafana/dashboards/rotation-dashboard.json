{
  "title": "XProxy Tester - IP Rotation Analysis",
  "tags": [
    "x-proxy-tester",
    "rotation"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "type": "stat",
      "title": "Proxies with Rotation",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT COUNT(*) as value FROM proxies WHERE active = true AND rotation_status = 'Rotating'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "type": "stat",
      "title": "Proxies with No Rotation",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT COUNT(*) as value FROM proxies WHERE active = true AND rotation_status = 'NoRotation'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "type": "stat",
      "title": "Total Rotations (24h)",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT SUM(rotation_count) as value FROM proxies WHERE active = true AND last_rotation_at >= DATE_SUB(NOW(), INTERVAL 24 HOUR)",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          }
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "type": "timeseries",
      "title": "Rotation Events Over Time",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  UNIX_TIMESTAMP(DATE_FORMAT(last_rotation_at, '%Y-%m-%d %H:%i:00')) as time_sec,\n  COUNT(*) as rotation_count\nFROM proxies\nWHERE active = true\n  AND last_rotation_at >= DATE_SUB(NOW(), INTERVAL 24 HOUR)\n  AND last_rotation_at IS NOT NULL\nGROUP BY time_sec\nORDER BY time_sec",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "type": "piechart",
      "title": "Rotation Status Distribution",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT rotation_status as metric, COUNT(*) as value FROM proxies WHERE active = true GROUP BY rotation_status",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "type": "table",
      "title": "Proxies with High Same IP Count (Potential Rotation Issues)",
      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "xproxy-tester-mysql"
          },
          "rawSql": "SELECT\n  name,\n  location,\n  last_ip,\n  same_ip_count,\n  rotation_count,\n  rotation_status,\n  last_rotation_at,\n  updated_at\nFROM proxies\nWHERE active = true\n  AND same_ip_count >= 10\nORDER BY same_ip_count DESC\nLIMIT 20",
          "format": "table",
          "refId": "A"
        }
      ]
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "editable": true,
